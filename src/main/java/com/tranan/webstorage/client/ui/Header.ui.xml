<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.s1 {
		position: fixed;
		top: 0;
		z-index: 1;
		background-color: rgba(83, 81, 81, 1);
		border-top: 0px solid rgba(83, 81, 81, 1);
		border-bottom: 0px solid rgba(83, 81, 81, 1);
		height: 75px;
		width: 100%;
	}
	
	.s2 {
		position: relative;
		margin: auto;
		width: 900px;
	}
	
	.s3 {
		position: relative;
		float: left;
		font-size: 20px;
		font-weight: bold;
		color: #D3D3D3;
		margin-top: 23px;
		letter-spacing: 0.3em;
		cursor: pointer;
		font-family: 'Ubuntu', sans-serif;
	}
	
	.s4 {
		position: relative;
		float: right;
		font-size: 16px;
		font-weight: normal;
		color: #D3D3D3;
		margin-top: 25px;
		cursor: pointer;
		margin-right: 70px;
		-webkit-transition: color .5s linear, background-color .5s ease-in-out;
		-moz-transition: color .5s linear, background-color .5s ease-in-out;
		-o-transition: color .5s linear, background-color .5s ease-in-out;
		transition: color .5s linear, background-color .5s ease-in-out;
	}
	
	.s4:hover {
		color: #A2C2C9;
		-webkit-transition: background-color .5s ease-in-out;
		-moz-transition: background-color .5s ease-in-out;
		-o-transition: background-color .5s ease-in-out;
		transition: background-color .5s ease-in-out;
	}
	
	.s4_active {
		position: relative;
		float: right;
		font-size: 17px;
		font-weight: normal;
		color: white;
		margin-top: 25px;
		cursor: pointer;
		margin-right: 70px;
	}
	
	.s5 {
		position: relative;
		float: right;
		font-size: 18px;
		font-weight: normal;
		color: #D3D3D3;
		margin-top: 24px;
		cursor: pointer;
		-webkit-transition: color .5s linear, background-color .5s ease-in-out;
		-moz-transition: color .5s linear, background-color .5s ease-in-out;
		-o-transition: color .5s linear, background-color .5s ease-in-out;
		transition: color .5s linear, background-color .5s ease-in-out;
	}
	
	.s5:hover {
		color: #A2C2C9;
		-webkit-transition: color .5s linear, background-color .5s ease-in-out;
		-moz-transition: color .5s linear, background-color .5s ease-in-out;
		-o-transition: color .5s linear, background-color .5s ease-in-out;
		transition: color .5s linear, background-color .5s ease-in-out;
	}
	
	.s6 {
		position: absolute;
		width: 100%;
		top: -24px;
		left: 0;
		text-align: center;
		background: #9E7076;
		font-size: 14px;
		padding-bottom: 2px;
		color: white;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.s1}">
		<g:HTMLPanel styleName="{style.s2}">
			<g:Label styleName="{style.s3}">PRETTY GAL</g:Label>

			<g:HTMLPanel styleName="{style.s5}">
				<g:Label ui:field="cartPage" styleName="{style.s6}">1</g:Label>
				<i class="material-icons">shopping_cart</i>
			</g:HTMLPanel>
			<g:Label ui:field="contactPage" styleName="{style.s4}">Contact</g:Label>
			<g:Label ui:field="shopPage" styleName="{style.s4}">Shop</g:Label>
			<g:Label ui:field="homePage" styleName="{style.s4_active}">Home</g:Label>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 