<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .CartItem_s1;
	.s1 {
		position: relative;
		overflow: hidden;
		margin-top: 35px;
		width: 900px;
	}
	
	.s2 {
		position: relative;
		overflow: hidden;
		width: 100%;
	}
	
	.s3 {
		position: relative;
		float: left;
		width: 152px;
		height: 152px;
	}
	
	.s4 {
		position: relative;
		float: left;
		margin-left: 35px;
		width: 380px;
		height: 170px;
	}
	
	.s5 {
		position: relative;
		float: left;
		margin-left: 35px;
	}
	
	.s6 {
		position: relative;
		float: left;
		margin-left: 35px;
	}
	
	.s7 {
		font-size: 20px;
		font-weight: 400;
		color: #303132;
		font-family: 'Raleway', sans-serif;
		margin-bottom: 10px;
	}
	
	.s8 {
		opacity: 0.5;
		color: #303132;
		font-weight: 400;
		margin-right: 10px;
		margin-left: 5px;
		font-size: 15px;
		font-family: 'Raleway', sans-serif;
		text-decoration: line-through;
		float: left;
	}
	
	.s9 {
		float: left;
		color: #303132;
		font-weight: 400;
		font-size: 15px;
		font-family: 'Raleway', sans-serif;
	}
	
	.s10 {
		position: absolute;
		bottom: 0;
		text-decoration: underline;
		color: #303132;
		cursor: pointer;
		font-size: 12px;
		font-family: 'Raleway', sans-serif;
		font-weight: 400;
	}
	
	.CartItem_s1 {
		text-align: center;
	}
	
	.s11 {
		color: #6CA1AD;
		font-size: 28px;
		margin-right: 8px;
		cursor: pointer;
		text-decoration: none !important;
	}
	
	.s12 {
		color: #6CA1AD;
		font-size: 20px;
		margin-left: 8px;
		cursor: pointer;
		text-decoration: none !important;
	}
	
	.s13 {
		float: right;
		color: #303132;
		font-weight: 400;
		font-size: 20px;
		font-family: 'Raleway', sans-serif;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.s1}">
		<g:HTMLPanel styleName="{style.s2}">
			<g:HTMLPanel styleName="{style.s3}">
				<g:Image ui:field="itemImg" width="100%" height="100%" url="Resources/6.png"></g:Image>
			</g:HTMLPanel>

			<g:HTMLPanel styleName="{style.s4}">
				<g:Label styleName="{style.s7}">I'm a Product</g:Label>
				<g:HTMLPanel>
					<g:Label styleName="{style.s9}">Price: </g:Label>
					<g:Label styleName="{style.s8}">$19.99 </g:Label>
					<g:Label styleName="{style.s9}">$14.99</g:Label>
				</g:HTMLPanel>
				<g:Anchor styleName="{style.s10}">Remove Item</g:Anchor>
			</g:HTMLPanel>

			<g:HTMLPanel styleName="{style.s5}">
				<g:Anchor styleName="{style.s11}">-</g:Anchor>
				<g:TextBox height="23px" width="25px" ui:field="txbQuantity">1</g:TextBox>
				<g:Anchor styleName="{style.s12}">+</g:Anchor>
			</g:HTMLPanel>

			<g:HTMLPanel>
				<g:Label styleName="{style.s13}">$14.99</g:Label>
			</g:HTMLPanel>
		</g:HTMLPanel>
		<div style="width: 100%; margin-top: 10px; border-bottom: 1px #D3D3D3 solid"></div>
	</g:HTMLPanel>
</ui:UiBinder> 