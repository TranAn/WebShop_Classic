<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .CategorieTable_s1, .CategorieTable_s1_actived, .CategorieTable_s2;
	.CategorieTable_s1 {
		position: relative;
		overflow: hidden;
		background-color: #ffffff;
		padding: 10px 15px;
		border-bottom: 1px solid #e2e9e6;
		cursor: pointer;
		color: #42a5f5;
	}
	
	.CategorieTable_s1 i {
		color: #42a5f5;
	}
	
	.CategorieTable_s1_actived {
		position: relative;
		overflow: hidden;
		background-color: #42a5f5;
		padding: 10px 15px;
		border-bottom: 1px solid #e2e9e6;
		cursor: pointer;
		color: #fff;
	}
	
	.CategorieTable_s1_actived i {
		color: #fff;
	}
	
	.CategorieTable_s2 {
		float: left;
		font-size: 22px;
		margin-right: 2px;
	}
	
	.s1 {
		float: left;
		font-size: 13px;
		margin-top: 2px;
		margin-right: 10px;
		font-weight: 500;
	}
	</ui:style>
	<g:HTMLPanel ui:field="row" styleName="CategorieTable_s1">
		<i class="material-icons CategorieTable_s2">&#xE315;</i>
		<g:Label styleName="{style.s1}" ui:field="text">General</g:Label>
	</g:HTMLPanel>
</ui:UiBinder> 