<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .ItemTableRow_s1, .ItemTableRow_s2, .ItemTableRow_s3;
	.s1 {
		position: relative;
		/* height: 70px; */
		width: 100%;
		background-color: #ffffff;
		border: 1px solid #e2e9e6;
		border-bottom: none;
		-webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
		-moz-box-sizing: border-box; /* Firefox, other Gecko */
		box-sizing: border-box; /* Opera/IE 8+ */ /* padding: 10px 15px; */
		overflow: hidden;
		display: table;
		padding: 5px 15px 5px 15px;
	}
	
	.s2 {
		position: relative;
		/* float: left; */
		margin: 9px 10px 0px 15px;
		width: 70px;
		display: table-cell;
		vertical-align: middle;
	}
	
	.s2_price {
		position: relative;
		/* float: left; */
		margin-top: 10px;
		margin-left: 20px;
		width: 130px;
		display: table-cell;
		vertical-align: middle;
		padding-right: 10px;
	}
	
	.s2_type {
		position: relative;
		/* float: left; */
		margin-top: 10px;
		padding-right: 35px;
		width: 130px;
		display: table-cell;
		vertical-align: middle;
	}
	
	.s2_quantity {
		position: relative;
		/* float: left; */
		margin: 10px 20px 0px 10px;
		width: 80px;
		display: table-cell;
		vertical-align: middle;
	}
	
	.s3 {
		overflow: hidden;
		width: 50px;
		height: 50px;
	}
	
	.s4 {
		font-size: 14px;
		/* line-height: 4.5; */
		cursor: pointer;
	}
	
	.s5 {
		/* margin-left: 20px; */
		float: left;
		font-size: 14px;
		line-height: 3.2;
	}
	
	.s6 {
		float: left;
		font-size: 12px;
		margin-left: 5px;
		line-height: 3.9;
	}
	
	.s7 {
		position: relative;
		/* float: left; */
		display: table-cell;
		vertical-align: middle;
		cursor: pointer;
	}
	
	.s7:hover {
		text-decoration: underline;
		color: #42a5f5;
	}
	
	.s8 {
		position: relative;
		/* float: right; */
		display: table-cell;
		vertical-align: middle;
		margin: 20px 20px 0px 10px;
		width: 30px;
		text-align: right;
		cursor: pointer;
		-webkit-transition: color .5s linear, background-color .5s ease-in-out;
		-moz-transition: color .5s linear, background-color .5s ease-in-out;
		-o-transition: color .5s linear, background-color .5s ease-in-out;
		transition: color .5s linear, background-color .5s ease-in-out;
	}
	
	.s8:hover {
		color: red;
		border: none;
		-webkit-transition: background-color .5s ease-in-out;
		-moz-transition: background-color .5s ease-in-out;
		-o-transition: background-color .5s ease-in-out;
		transition: background-color .5s ease-in-out;
		webkit-transition: box-shadow 0.28s cubic-bezier(0.4, 0, 0.2, 1);
		transition: box-shadow 0.28s cubic-bezier(0.4, 0, 0.2, 1);
	}
	
	.s9 {
		font-size: 14px;
		line-height: 3.2;
		text-align: left;
		padding-left: 20px;
	}
	
	.s10 {
		overflow: hidden;
		margin-top: -10px;
		padding: 5px;
	}
	
	.s11 {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
	}
	
	.ItemTableRow_s1 {
		border-radius: 40px;
	}
	
	.ItemTableRow_s2 {
		overflow: hidden;
		margin-top: 8px;
	}
	
	.ItemTableRow_s3 {
		text-align: center;
		margin-right: 5px;
		font-size: 14px;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.s1}" ui:field="itemRow">

		<g:HTMLPanel styleName="{style.s2}">
			<g:Image styleName="{style.s3}" ui:field="itemImg"></g:Image>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="{style.s7}" ui:field="itemName">
			<g:Label ui:field="nameText" styleName="{style.s4}"></g:Label>
			<g:Anchor ui:field="itemViewButton" styleName="{style.s11}"></g:Anchor>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="{style.s2_price}">
			<g:Label ui:field="costText" styleName="{style.s5}">0</g:Label>
			<!-- <g:Label styleName="{style.s6}">VND</g:Label> -->
		</g:HTMLPanel>

		<g:HTMLPanel styleName="{style.s2_price}">
			<g:Label ui:field="priceText" styleName="{style.s5}">0</g:Label>
			<!-- <g:Label styleName="{style.s6}">VND</g:Label> -->
		</g:HTMLPanel>

		<g:HTMLPanel styleName="{style.s2_type}">
			<g:HTMLPanel ui:field="typeTable" styleName="{style.s10}">
				<!-- <g:HTMLPanel styleName="ItemTableRow_s2">
					<g:Label styleName="ItemTableRow_s3">Default: </g:Label>
					<g:Label styleName="ItemTableRow_s3">10</g:Label>
				</g:HTMLPanel>

				<g:HTMLPanel styleName="ItemTableRow_s2">
					<g:Label styleName="ItemTableRow_s3">Vang:
					</g:Label>
					<g:Label styleName="ItemTableRow_s3">10</g:Label>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="ItemTableRow_s2">
					<g:Label styleName="ItemTableRow_s3">Do:
					</g:Label>
					<g:Label styleName="ItemTableRow_s3">10</g:Label>
				</g:HTMLPanel> -->
			</g:HTMLPanel>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="{style.s2_quantity}">
			<g:Label ui:field="saleText" styleName="{style.s9}">0</g:Label>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="{style.s8}">
			<i class="material-icons">&#xE872;</i>
			<g:Anchor ui:field="deleteButton" styleName="{style.s11}"></g:Anchor>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 