	@external .ItemImportTable_s1, .ItemImportTable_s1Right, .ItemImportTable_s1_active, .ItemImportTable_s2, .ItemImportTable_s3, .ItemImportTable_s4, .ItemImportTable_s5, .ItemImportTable_s6, .ItemImportTable_s7, .ItemImportTable_s8, .ItemImportTable_s9,.ItemImportTable_s10, .ItemImportTable_s11, .ItemImportTable_s12, .ItemImportTable_s13;
	.s1 {
		position: relative;
		overflow: hidden;
		width: 100%;
		display: table;
		/* border: 1px #e2e9e6 solid; */
	}
	
	.ItemImportTable_s1 {
		position: relative;
		float: left;
		height: 70px;
		background-color: #ffffff;
		outline: 1px solid #e2e9e6;
		margin-right: 0px;
		display: table;
		cursor: pointer;
		-webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
		-moz-box-sizing: border-box; /* Firefox, other Gecko */
		box-sizing: border-box; /* Opera/IE 8+ */
		-webkit-transition: color .10s linear, background-color .10s ease-in-out;
		-moz-transition: color .10s linear, background-color .10s ease-in-out;
		-o-transition: color .10s linear, background-color .10s ease-in-out;
		transition: color .10s linear, background-color .10s ease-in-out;
	}
	
	.ItemImportTable_s1:hover {
		background: #efefef;
		-webkit-transition: color .10s linear, background-color .10s ease-in-out;
		-moz-transition: color .10s linear, background-color .10s ease-in-out;
		-o-transition: color .10s linear, background-color .10s ease-in-out;
		transition: color .10s linear, background-color .10s ease-in-out;
	}
	
	.ItemImportTable_s1Right {
		position: relative;
		float: left;
		height: 70px;
